{
  "name": "岚",
  "description": "极简的新标签页插件，献给喜欢壁纸和极简风格的你。",
  "version": "1.0.9",
  "manifest_version": 3,
  "permissions": ["storage", "downloads", "unlimitedStorage", "bookmarks"],
  "host_permissions": ["https://cn.bing.com/"],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "bg.js"
  }
}
